{
  "name": "trading-chart",
  "version": "1.0.0",
  "description": "Real-time trading chart application for TSX Trading Bot V5",
  "main": "dist/backend/server.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build:frontend": "webpack --mode production",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nodemon --watch src/backend --ext ts --exec \"node --loader ts-node/esm\" src/backend/server.ts",
    "dev:frontend": "webpack serve --mode development",
    "start": "node dist/backend/backend/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx}",
    "clean": "rimraf dist",
    "test-data": "node publish-test-data.js",
    "diagnose": "node diagnose-data-flow.js",
    "fetch-history": "node fetch-historical-data.js",
    "redis-listen": "node test-redis-listen.js"
  },
  "keywords": ["trading", "chart", "candlestick", "websocket", "redis"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.21.2",
    "ioredis": "^5.6.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "lightweight-charts": "^4.2.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "axios": "^1.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.2.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.18.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "webpack": "^5.100.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}
