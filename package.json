{
  "name": "tsx-trading-bot-v5",
  "version": "5.0.0",
  "description": "TSX Trading Bot V5 - Clean architecture automated trading system for TopStepX",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node scripts/start-all.js",
    "start:dev": "NODE_ENV=development npm start",
    "start:prod": "NODE_ENV=production npm start",
    "stop": "node scripts/stop-all.js",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "validate:config": "node scripts/validate-config.js",
    "monitor": "node src/core/monitoring/dashboard.js",
    "aggregator": "node src/core/aggregator/start-aggregator-production.js",
    "connection-manager": "node connection-manager/index.js",
    "control-panel": "node src/ui/control-panel/server.js",
    "manual-trading": "node manual-trading/server.js",
    "config-ui": "node src/ui/config/server.js",
    "clean": "rimraf logs temp coverage",
    "prebuild": "npm run clean",
    "build": "npm run validate:config && npm run lint",
    "security-audit": "npm audit && npm audit fix",
    "update-deps": "npm update && npm audit fix"
  },
  "keywords": [
    "trading",
    "bot",
    "topstep",
    "automated-trading",
    "risk-management"
  ],
  "author": "TSX Trading Bot Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "ajv": "^8.12.0",
    "axios": "^1.6.7",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.11.0",
    "js-yaml": "^4.1.0",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "redis": "^4.7.1",
    "socket.io": "^4.8.1",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/[your-username]/TSX-Trading-Bot-V5.git"
  },
  "bugs": {
    "url": "https://github.com/[your-username]/TSX-Trading-Bot-V5/issues"
  },
  "homepage": "https://github.com/[your-username]/TSX-Trading-Bot-V5#readme"
}